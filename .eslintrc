{
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
      "max-len": "off",
      "brace-style": ["error", "stroustrup"],
      "indent": ["error", 4],
      "prefer-promise-reject-errors": "off",
      "prefer-rest-params": "off"
    }
  }