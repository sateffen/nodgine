{
    "basepath": "./",
    "globalize": true,
    "loadpaths": [
        "./"
    ],
    "routes": [
        {
            "route": "/nodginetestroute",
            "controller": "testroutecontroller.js"
        }
    ],
    "services": [
        {
            "id": "testId",
            "type": "testType",
            "controller": "testservice.js"
        }
    ],
    "preprocessors": [
        "testpreprocessor.js"
    ],
    "postprocessors": [
        "testpostprocessor.js"
    ],
    "http": {
        "port": 1235
    },
    "https": {
        "key": "../httpsfiles/localhost.key",
        "cert": "../httpsfiles/localhost.cert",
        "port": 1236
    },
    "logger": {
        "writeToFile": false,
        "writeToConsole": true,
        "minimumLogLevel": "WARNING",
        "logFile": "test.log"
    }
}
